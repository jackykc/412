        type="cv_camera_node" name="cv_camera" >
    <param name="rate" type="double" value="30" />
    <!-- <param name="image_width" type="int" value="640" />
    <param name="image_height" type="int" value="480" /> -->
    <param name="frame_id" value="front_camera" />
