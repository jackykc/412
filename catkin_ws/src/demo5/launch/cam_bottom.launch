    <remap from="/cv_camera/image_raw" to="/bottom/rgb/image_raw" />
    <!-- <param name="camera_info_url" type="string" value="file://$(find demo5)/front_camera.yaml" /> -->
  </node>

  <!-- <group ns="camera/rgb">	
    <node pkg="image_proc" type="image_proc" name="rectify_color" respawn="true" />
  </group>   -->
</launch>